# DevMind example environment variables
# Copy this file to .env and fill with your own keys. Do NOT commit real keys.

# Provider API keys
GEMINI_API_KEY=your_gemini_key_here
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Ollama (local model) base URL (no API key required by default)
OLLAMA_BASE_URL=http://localhost:11434

# Debug flags
# Set to 'true' to enable verbose dev logging and keep terminal output
VITE_DEBUG=false

# Dev proxy: when true, browser LLM requests are routed through the dev server
# to avoid exposing API keys to the client and to avoid CORS issues.
# Disable in production.
VITE_GEMINI_DEV_PROXY=false

# Deprecated: `API_KEY` (legacy alias) â€” prefer `GEMINI_API_KEY` as the single source of truth.
# Leave empty for clarity; do not rely on this alias.
API_KEY=
